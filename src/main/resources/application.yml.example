spring:
  application:
    name: "POC for java spring-boot swagger"
  datasource:
    url: <your postgres url>
    username: postgres
    password: <your password>
    driver-class-name: org.postgresql.Driver
  jpa:
    hibernate:
      ddl-auto: create
springdoc:
  api-docs:
    path: /api-docs
  swagger-ui:
    operations-sorter: method
server:
  error:
    include-message: always
  port: 3300
